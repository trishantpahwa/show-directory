{
  "name": "show-directory",
  "version": "1.0.0",
  "description": "A package to list files and folders in a directory.",
  "main": "dist/index.js",
  "scripts": {
    "test": "nodemon -w src/index.js --exec \"babel-node --presets es2015,stage-0 src/index.js\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trishantpahwa/show-directory.git"
  },
  "author": "Trishant Pahwa <trishantpahwa@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trishantpahwa/show-directory/issues"
  },
  "homepage": "https://github.com/trishantpahwa/show-directory#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^6.5.1",
    "nodemon": "^1.19.4",
    "rimraf": "^3.0.0"
  }
}
